package day7;

public class CompararableComparator {
	
	int x, y;
	public CompararableComparator(int a, int b){
		init(a,b);
	}
	
	public void init(int x , int y) {
		this.x= x;
		this.y = y;
	}
	
	
	public static void main(String[] args) {
		
	int x =10;
	int y =20;
	
	CompararableComparator co = new  CompararableComparator(x,y);
	System.out.println( x +":"+y);
	
		/*
		
		System.out.println(new String("Hello")instanceof String);
		System.out.println("Hello"instanceof String);
		System.out.println(new String("Hello")instanceof Object);
		System.out.println("Hello"instanceof Object);
		System.out.println();
		System.out.println();
		System.out.println();
		
		*/
		
	}


}
